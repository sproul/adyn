< 	if (val==null)
< 	{
< 	    val = ""
< 	}
< 	else
< 	{
< 	    val = " value='" + val + "'"
< 	} 
< 	document.write("<tr><td width=90><b>" + label + "</b></td><td><input type=" + type + " name=" + name + val + " size=40></td><td><b>" + instructions + "</b></td></tr>") 
<     } 
< }
